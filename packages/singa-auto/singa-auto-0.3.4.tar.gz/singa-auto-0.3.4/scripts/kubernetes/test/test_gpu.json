{
    "kind": "Deployment",
    "apiVersion": "apps/v1",
    "metadata": {
        "labels": {
            "name": "singa-auto-test-gpu"
        },
        "name": "singa-auto-test-gpu1"
    },
    "spec": {
        "selector": {
            "matchLabels": {
                "name": "singa-auto-test-gpu"
            }
        },
        "template": {
            "metadata": {
                "labels": {
                    "name": "singa-auto-test-gpu"
                }
            },
            "spec": {
                "containers": [
                    {
                        "name": "singa-auto-test-gpu",
                        "image": "torchtest:1",
                        "resources": {"limits": {"nvidia.com/gpu":  1}}
                    }
                ]
            }
        },
        "replicas": 1
    }
}
